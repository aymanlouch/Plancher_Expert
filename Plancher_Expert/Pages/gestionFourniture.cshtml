@page
@model gestionFournitureModel
@{
}
<body class="sub_page">

    <!-- fourniture section -->
    <section class="furniture_section layout_padding">
        <div class="container">
            <div class="heading_container">
                <h2>
                    Gestion des Fournitures
                </h2>
            </div>
            <form method="post" name="flooringType">
                <div class="row">
                    @foreach (var frnt in Model.fournitureList)
                    {
                        <div class="col-md-6 col-lg-4">
                            <div class="box">
                                <div class="detail-box">
                                    <h5>
                                        @frnt.FloorType
                                    </h5>
                                    <div class="price_box">
                                        <h6 class="price_heading">
                                            <span>$</span> @frnt.MaterialRate /m²
                                        </h6>
                                        <a href="/updateFourniture?IdP=@frnt.Id">
                                            Modifier
                                        </a>
                                        <a href="/deleteFourniture?IdP=@frnt.Id">
                                            Supprimer
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    }
                </div>
            </form>
        </div>
    </section>

    <!-- end furniture section -->
    <!-- jQery -->
    <script src="js/jquery-3.4.1.min.js"></script>
    <!-- bootstrap js -->
    <script src="js/bootstrap.js"></script>
    <!-- custom js -->
    <script src="js/custom.js"></script>
    <!-- Google Map -->
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCh39n5U-4IoWpsVGUHWdqB6puEkhRLdmI&callback=myMap"></script>
    <!-- End Google Map -->

</body>
<footer class="border-top footer text-muted">
    <div class="container">
        &copy; 2023 - Plancher_Expert - <a asp-area="" asp-page="/Privacy">Privacy</a>
    </div>
</footer>
